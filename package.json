{
  "name": "nuxt-hedge",
  "type": "module",
  "version": "0.4.0",
  "packageManager": "pnpm@7.8.0",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-hedge#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-hedge.git",
    "directory": "packages/zhead"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-hedge/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"**/*.{ts,vue,json,yml}\"",
    "build": "nuxi prepare playground && nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "vitest basic"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.11",
    "@vueuse/head": "1.0.0-rc.3",
    "@zhead/schema-vue": "^0.8.5",
    "@zhead/vue": "^0.8.5",
    "fast-glob": "^3.2.12",
    "pathe": "^0.3.9",
    "zhead": "^0.8.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@nuxt/module-builder": "latest",
    "@nuxt/schema": "^3.0.0-rc.11",
    "@nuxt/test-utils": "3.0.0-rc.11",
    "@nuxtjs/eslint-config-typescript": "latest",
    "bumpp": "^8.2.1",
    "cheerio": "1.0.0-rc.12",
    "eslint": "8.25.0",
    "nuxt": "3.0.0-rc.11",
    "playwright": "^1.27.0",
    "sass": "^1.55.0",
    "vitest": "^0.24.0"
  }
}
